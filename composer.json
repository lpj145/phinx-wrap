{
    "name": "madantas/phinx-wrapper",
    "description": "This package simply phinx configuration by passing env vars to commands or base you app console to inject config by constructor.",
    "type": "library",
    "keywords": ["phinx", "wrapper", "phinx-wrapper", "migrations"],
    "version": "1.0.0",
    "require": {
      "robmorgan/phinx": "0.10.6",
      "vlucas/phpdotenv": "3.3.3",
      "composer/composer": "1.8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Dantas",
            "email": "marcos.adantas@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "mdantas\\PhinxWrap\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^8.2@dev"
    },
    "scripts": {
        "post-install-cmd": "mdantas\\PhinxWrap\\ComposerInstall::postInstallCmd",
        "post-package-install": "mdantas\\PhinxWrap\\ComposerInstall::postPackageInstall",
        "post-autoload-dump": "mdantas\\PhinxWrap\\ComposerInstall::postAutoloadDump"
    },
    "bin": [
        "bin/phinxwrap"
    ]
}
